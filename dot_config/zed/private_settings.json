{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "features": {
    "inline_completion_provider": "none"
  },
  "autosave": "on_focus_change",
  "buffer_font_size": 16,
  "buffer_font_family": "3270 Nerd Font Mono",
  "buffer_line_height": "standard",
  "gutter": {
    "line_numbers": false
  },
  "languages": {
    "JSON": {
      "format_on_save": "off"
    },
    "YAML": {
      "format_on_save": "off"
    }
  },
  "lsp": {
    "rust-analyze": {
      "binary": { "path_lookup": true }
    }
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "font_family": "3270 Nerd Font Mono",
    "line_height": "standard"
  },
  "theme": "Atelier Seaside Dark",
  "ui_font_size": 16,
  "vim_mode": true
}
